{
  "name": "compat-mode-examples",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "napi build --no-js --features latest",
    "build-napi3": "napi build --no-js --features napi3",
    "build-aarch64": "napi build --no-js --features latest --target aarch64-unknown-linux-gnu",
    "build-armv7": "napi build --no-js --features latest --target armv7-unknown-linux-gnueabihf",
    "build-i686": "napi build --no-js --features latest --target i686-pc-windows-msvc",
    "build-i686-release": "napi build --no-js --release --features latest --target i686-pc-windows-msvc",
    "build-release": "napi build --no-js --features latest --release",
    "test": "node ./index.js"
  },
  "devDependencies": {
    "napi-cli": "workspace:*"
  }
}
